
<mujoco model="scene">

    <compiler autolimits="true" angle="degree"/>
    <option integrator="implicitfast" timestep="0.001">
      <flag multiccd="enable"/>
    </option>
    <visual>
      <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
      <rgba haze="0.15 0.25 0.35 1"/>
      <global fovy="58" azimuth="0" elevation="90"/>
    </visual>
    <statistic center="0.13125 0.1407285 1.5" extent="0.85"/>
  
    <asset>
      <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
      <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
        markrgb="0.8 0.8 0.8" width="300" height="300"/>

      <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
      <material name="collision_material" rgba="0 0 0 0"/> <!-- transparent for collision -->
      <material name="visual_material" rgba="0 0 1 0.3"/> <!-- Blue semi-transparent for visual -->

      <texture name="crescent" file="texture.png" type="2d"/>
      <material name="crescent" texture="crescent" specular="0.5" shininess="0.5"/>
      <mesh file="crescent.obj" scale="1 1 1"/>
      
      <texture name="cross" file="texture.png" type="2d"/>
      <material name="cross" texture="cross" specular="0.5" shininess="0.5"/>
      <mesh file="cross.obj" scale="1 1 1"/>

      <texture name="diamond" file="texture.png" type="2d"/>
      <material name="diamond" texture="diamond" specular="0.5" shininess="0.5"/>
      <mesh file="diamond.obj" scale="1 1 1"/>

      <texture name="heart" file="texture.png" type="2d"/>
      <material name="heart" texture="heart" specular="0.5" shininess="0.5"/>
      <mesh file="heart.obj" scale="1 1 1"/>

      <texture name="hexagon" file="texture.png" type="2d"/>
      <material name="hexagon" texture="hexagon" specular="0.5" shininess="0.5"/>
      <mesh file="hexagon.obj" scale="1 1 1"/>

      <texture name="parallelogram" file="texture.png" type="2d"/>
      <material name="parallelogram" texture="parallelogram" specular="0.5" shininess="0.5"/>
      <mesh file="parallelogram.obj" scale="1 1 1"/>

      <texture name="semicircle" file="texture.png" type="2d"/>
      <material name="semicircle" texture="semicircle" specular="0.5" shininess="0.5"/>
      <mesh file="semicircle.obj" scale="1 1 1"/>

      <texture name="shuriken" file="texture.png" type="2d"/>
      <material name="shuriken" texture="shuriken" specular="0.5" shininess="0.5"/>
      <mesh file="shuriken.obj" scale="1 1 1"/>

      <texture name="star" file="texture.png" type="2d"/>
      <material name="star" texture="star" specular="0.5" shininess="0.5"/>
      <mesh file="star.obj" scale="1 1 1"/>

      <texture name="trapezium" file="texture.png" type="2d"/>
      <material name="trapezium" texture="trapezium" specular="0.5" shininess="0.5"/>
      <mesh file="trapezium.obj" scale="1 1 1"/>

      <texture name="triangle" file="texture.png" type="2d"/>
      <material name="triangle" texture="triangle" specular="0.5" shininess="0.5"/>
      <mesh file="triangle.obj" scale="1 1 1"/>

      <texture name="disc" file="texture.png" type="2d"/>
      <material name="disc" texture="disc" specular="0.5" shininess="0.5"/>
      <mesh file="disc.obj" scale="1 1 1"/>
    </asset>
  
    <worldbody>
      <light pos="0 0 1.5" dir="0 0 -1" directional="true"/>
      <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>

      
      <body name="crescent" pos="0 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="crescent" material="crescent" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="cross" pos="0 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="cross" material="cross" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="diamond" pos="0.1 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="diamond" material="diamond" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="heart" pos="0.2 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="heart" material="heart" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="hexagon" pos="0.3 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="hexagon" material="hexagon" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="parallelogram" pos="0.4 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="parallelogram" material="parallelogram" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="semicircle" pos="0.5 0 0.2">
        <freejoint/>
        <geom type="mesh" mesh="semicircle" material="semicircle" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="shuriken" pos="0 0.1 0.2">
        <freejoint/>
        <geom type="mesh" mesh="shuriken" material="shuriken" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="star" pos="0 0.2 0.2">
        <freejoint/>
        <geom type="mesh" mesh="star" material="star" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="trapezium" pos="0 0.3 0.2">
        <freejoint/>
        <geom type="mesh" mesh="trapezium" material="trapezium" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="triangle" pos="0 0.4 0.2">
        <freejoint/>
        <geom type="mesh" mesh="triangle" material="triangle" euler="90 0 0" mass="0.1" condim="6"/>
      </body>

      <body name="disc" pos="0 0.4 0.2">
        <freejoint/>
        <geom type="mesh" mesh="disc" material="disc" euler="90 0 0" mass="0.1" condim="6"/>
      </body>
    </worldbody>
  </mujoco>
